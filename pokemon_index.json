pokedex_data = json.loads(json_data_string)
simple_pokedex_list = []

# Keep track of which Pokédex numbers we've already added
seen_numbers = set()

# Iterate over the values (the Pokémon dictionaries)
for poke_info in pokedex_data.values():
    poke_num = poke_info.get('num')
    poke_name = poke_info.get('name')

    # Exclude forms (Mega, Gmax, Alola, etc.) by checking for the base species and number
    # This ensures only one entry per number, keeping the list clean.
    if poke_num not in seen_numbers and '-' not in poke_name:
        simple_pokedex_list.append({
            "number": poke_num,
            "name": poke_name
        })
        seen_numbers.add(poke_num)
        
print(simple_pokedex_list)